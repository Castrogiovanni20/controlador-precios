{
  "expo": {
    "name": "Comparador de Precios",
    "slug": "proyecto-barras",
    "platforms": ["ios", "android", "web"],
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0288d1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xifra.controlpreciosmaximos",
      "buildNumber": "1.3.0"
    },
    "android": {
      "package": "com.xifra.controlpreciosmaximos",
      "versionCode": 4,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA"
      ]
    }
  }
}
